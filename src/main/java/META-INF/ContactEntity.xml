<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="system.dao.entity.ContactEntity">
        <table name="contact" schema="Contacts"/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="name">
                <column name="name" length="33"/>
            </basic>
            <basic name="number">
                <column name="number" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
